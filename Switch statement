#include<stdio.h>
int main(){
    int age;
    printf("Enter the age: ");
    scanf("%d", &age);
    switch(age){
        case 1:
        printf("age is between 1 to 15\n");
        break;
        case 2:
        printf("age is between 16 to 30\n");
        break;
        case 3:
        printf("age is between 31 to 60\n");
        break;
        case 4:
        printf("you are senior citizen\n");
        break;
        default:
        printf("Enter valid case number");
    }
    return 0;
}
